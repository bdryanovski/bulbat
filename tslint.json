{
   "extends": ["tslint:recommended", "tslint-config-prettier"],
   "rules": {
      "interface-name": [true, "never-prefix"],
      "no-console": {
         "severity": "warning",
         "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
      },
      "object-literal-sort-keys": false,
      "ordered-imports": false
   }
}